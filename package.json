		"dev:build-client": "NODE_ENV=development webpack --mode development --config webpack.client.babel.js --watch",
		"prd:build-client": "NODE_ENV=production BABEL_ENV=production webpack --mode production --config webpack.client.babel.js",
		"prd:build-server": "NODE_ENV=production BABEL_ENV=production webpack --mode production --config webpack.server.babel.js",
